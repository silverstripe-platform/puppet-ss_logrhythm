[sources.syslog]
type = "syslog"
address = "<%= @listen_ip %>:<%= @listen_port %>"
mode = "<%= @listen_mode %>"

[sinks.kinesis]
type = "aws_kinesis_streams"
inputs = [ "syslog" ]
compression = "none"
region = "${REGION}"
stream_name = "${STREAM_NAME}"
auth.assume_role = "${ROLE_ARN}"
encoding.codec = "ndjson"